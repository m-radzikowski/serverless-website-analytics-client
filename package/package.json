{
  "name": "serverless-website-analytics-client",
  "version": "0.0.0",
  "description": "Web client for serverless-website-analytics",
  "author": "Rehan van der Merwe (https://twitter.com/der_rehan)",
  "license": "GPL-2.0-only",
  "main": "index.js",
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "generate-api-types": "wireit",
    "package": "wireit"
  },
  "wireit": {
    "generate-api-types": {
      "command": "esr scripts.ts -c generate-api-types",
      "files": [
        "src/OpenAPI-Ingest.yaml"
      ],
      "output": [
        "ingest-api-types.ts"
      ]
    },
    "package": {
      "command": "esr scripts.ts -c package",
      "files": [
        "scripts.ts",
        "src/**/*.ts",
        "OpenAPI-Ingest.yaml"
      ],
      "output": [
        "dist/**"
      ]
    }
  },
  "dependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.6",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/node": "^18.14.0",
    "@types/yargs": "^17.0.22",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "esbuild": "^0.17.10",
    "esbuild-runner": "^2.2.2",
    "execa": "5.1.1",
    "nanoid": "^4.0.1",
    "semantic-release": "19.0.5",
    "swagger-typescript-api": "^12.0.3",
    "typescript": "^4.9.5",
    "wireit": "^0.9.5",
    "yargs": "^17.7.1"
  }
}
